@model List<OrderVM>
@{
    if (Model.Count == 0)
    {
        <p>You have no orders.</p>
        <a asp-controller="Order" asp-action="PlaceOrder" class="btn btn-primary">Place new oeder</a>
    }
    else
    {
        foreach (var order in Model)
        {
            @Html.Partial("_OrderItem", order)
        }
    }
}


