@model BookDetailsViewModel

@{
    ViewData["Title"] = "Details";
}

<h2>Details</h2>
<p>Title: @Model.Title</p>
<p>Year: @Model.Year</p>
<p>Genre: @Model.BookGenre</p>
<p>Authors:</p>
<ol>
    @for (int i = 0; i < Model.AuthorsFullNames.Count; i++)
    {
        <li>@Model.AuthorsFullNames[i]</li>
    }
</ol>

