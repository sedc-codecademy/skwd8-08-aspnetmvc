<div>
    <table class="table-striped">
        <thead>
            <tr>
                <td>No.</td>
                <td>Day</td>
            </tr>
        </thead>
        <tbody>
            @for (int i = 1; i < 100; i++)
            {
                <tr>
                    <td>
                        #@i
                    </td>
                    <td>
                        @DateTime.Now.AddDays(i).Date.ToString("dd.MM.yyyy")
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>